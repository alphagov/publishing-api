# Ignore bundler config.
/.bundle

# Ignore all logfiles and tempfiles.
/log/*
!/log/.keep
/tmp
/coverage
/spec/reports
spec/examples.txt

# Ignore the generated pact
spec/pacts/publishing_api-content_store.json

# Ignore data for #live_content scripts
/script/live_content/data/*
!/script/live_content/data/.keep
