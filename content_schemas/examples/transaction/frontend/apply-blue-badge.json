        "document_type": "transaction",
        "schema_name": "transaction",
        "base_path": "/apply-blue-badge",
        "description": "Apply for or renew a Blue Badge, amend or cancel a saved application, track your application",
        "api_path": "/api/content/apply-blue-badge",
        "withdrawn": false,
        "content_id": "583e83bf-077e-4892-b8f0-52bfdf286fac",
        "locale": "en",
        "api_url": "http://www.dev.gov.uk/api/content/apply-blue-badge",
        "web_url": "http://www.dev.gov.uk/apply-blue-badge",
