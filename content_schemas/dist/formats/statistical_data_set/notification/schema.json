    "government": {
      "description": "DEPRECATED: Content should be associated with a government through a link",
      "type": "object",
      "required": [
        "title",
        "slug",
        "current"
      ],
      "additionalProperties": false,
      "properties": {
        "current": {
          "description": "Is the government that published this document still the current government.",
