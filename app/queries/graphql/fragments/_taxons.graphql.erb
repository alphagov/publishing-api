<%= render "fragments/parent_and_root_taxons" %>

fragment Taxons on Links {
  taxons: links_of_type(type: taxons) {
    api_path
    api_url
    base_path
    content_id
    description
    details {
      internal_name
      notes_for_editors
      visible_to_departmental_editors
    }
    document_type
    links {
      ...ParentAndRootTaxons
    }
    locale
    phase
    public_updated_at
    schema_name
    title
    web_url
    withdrawn
  }
}